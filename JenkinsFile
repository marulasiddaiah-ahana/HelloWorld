pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Welcome to Jenkins Automation'
            }
        }
    
    
        stage('Build') {
            steps {
                cd 
                ls
            }
        }
    
    
        stage('Deploy') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
